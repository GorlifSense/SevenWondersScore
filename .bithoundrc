{
  "critics": {
    "lint": {
      "engine": "tslint"
    }
  },
  "dependencies": {
    "unused-ignores": [
      "http-server",
      "ie-shim",
      "angular2-template-loader",
      "@angularclass/conventions-loader",
      "@angular/platform-server"
    ]
  },
  "test": [
    "**/test/**",
    "**/tests/**",
    "**/spec/**",
    "**/specs/**",
    "**.spec.**",
    "**.e2e.**"
  ],
  "ignore": [
    "**/deps/**",
    "**/node_modules/**",
    "**/thirdparty/**",
    "**/third_party/**",
    "**/vendor/**",
    "**/**-min-**",
    "**/**-min.**",
    "**/**.min.**",
    "**/**jquery.?(ui|effects)-*.*.?(*).?(cs|j)s",
    "**/**jquery-*.*.?(*).?(cs|j)s",
    "**/prototype?(*).js",
    "**/mootools*.*.*.js",
    "**/dojo.js",
    "**/MochiKit.js",
    "**/yahoo-*.js",
    "**/yui*.js",
    "**/ckeditor*.js",
    "**/tiny_mce*.js",
    "**/tiny_mce/?(langs|plugins|themes|utils)/**",
    "**/MathJax/**",
    "**/shBrush*.js",
    "**/shCore.js",
    "**/shLegacy.js",
    "**/modernizr.custom.?(*).js",
    "**/knockout-*.*.*.debug.js",
    "**/extjs/*.js",
    "**/extjs/*.xml",
    "**/extjs/*.txt",
    "**/extjs/*.html",
    "**/extjs/*.properties",
    "**/extjs/.sencha",
    "**/extjs/docs/**",
    "**/extjs/builds/**",
    "**/extjs/cmd/**",
    "**/extjs/examples/**",
    "**/extjs/locale/**",
    "**/extjs/packages/**",
    "**/extjs/plugins/**",
    "**/extjs/resources/**",
    "**/extjs/src/**",
    "**/extjs/welcome/**",
    "config/**"
  ]
}
