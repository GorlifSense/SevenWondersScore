{
  "name": "seven-wonders-score",
  "description": "Save your score points for 7 Wonders Board Games in Angular 2 way",
  "version": "0.0.1-alpha.4",
  "license": "ISC",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "tslint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
    "remark-lint": "remark *.md docs/*.md -u remark-lint",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "protractor": "protractor",
    "ci": "npm run lint && npm run remark-lint && npm test && npm run e2e",
    "codeclimate": "codeclimate-test-reporter < ./coverage/lcov.info",
    "codecov": "bash <(curl -s https://codecov.io/bash)",
    "edit-changelog": "vim CHANGELOG.md && git add CHANGELOG.md",
    "preversion": "npm test",
    "version": "npm run build && npm run edit-changelog",
    "postversion": "git push && git push --tags",
    "unstaged": "git status --porcelain"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/platform-server": "^4.0.0",
    "@angular/router": "^4.0.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.1.0",
    "typescript": "^2.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@types/jasmine": "^2.5.46",
    "@types/node": "^6.0.60",
    "codeclimate-test-reporter": "^0.4.0",
    "codelyzer": "~2.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "pre-commit": "^1.2.2",
    "protractor": "~5.1.0",
    "remark-cli": "^3.0.0",
    "remark-lint": "^6.0.0",
    "ts-node": "~2.0.0",
    "tslint": "^4.5.0",
    "typescript": "^2.1.0"
  },
  "peerDependencies": {
    "board-games-results": "^0.1.6"
  },
  "main": "src/main.ts",
  "engines": {
    "node": ">= 6",
    "npm": ">= 3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GorlifSense/SevenWondersScore.git"
  },
  "keywords": [
    "board",
    "games",
    "wonders",
    "save",
    "results",
    "angular2",
    "web",
    "app"
  ],
  "author": "Gorlif Sense <git@gorlifsense.com> (http://gorlifsense.com)",
  "bugs": {
    "url": "https://github.com/GorlifSense/SevenWondersScore/issues"
  },
  "pre-commit": [
    "remark-lint",
    "lint",
    "unstaged"
  ]
}
